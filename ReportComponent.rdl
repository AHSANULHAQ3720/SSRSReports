          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ParamQTY">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DISTINCT
	 CASE 
        WHEN Bonus &gt;= 0 AND Bonus &lt; 150 THEN 'Less than 150'
        ELSE 'Greater than 150'
    END AS BonusCategorization

  FROM [AdventureWorks2022].[Sales].[SalesPerson]</CommandText>
      </Query>
