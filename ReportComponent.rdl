  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ReportParam">
            <Value>=Parameters!ReportParam.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
    [BusinessEntityID],
    [TerritoryID],
    [SalesQuota],
    [Bonus],
    [CommissionPct],
    [SalesYTD],
    [SalesLastYear],
   CASE 
        WHEN Bonus &gt;= 0 AND Bonus &lt; 150 THEN 'Less than 150'
        WHEN Bonus &gt;= 150 AND Bonus &lt; 350 THEN 'Less than 350'
        ELSE 'Above 350'
    END AS BonusCategorization
FROM 
    [AdventureWorks2022].[Sales].[SalesPerson]
WHERE @ReportParam IN
    (
        CASE 
        WHEN Bonus &gt;= 0 AND Bonus &lt; 150 THEN 'Less than 150'
        WHEN Bonus &gt;= 150 THEN 'Greater than 150'
        ELSE 'None' END
    )</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
